##=== TEST.evalrbaa.report - Report desc for sra tests ----*- perl -*-===##
#
# This file defines a report to be generated for the sra test.
#
##===----------------------------------------------------------------------------------------===##

# Sort by name
$SortCol = 1;
$TrimRepeatedPrefix = 1;

# These are the columns for the report.  The first entry is the header for the
# column, the second is the regex to use to match the value.  Empty list create
# seperators, and closures may be put in for custom processing.
(
# Name
 ["Name" , '\'([^\']+)\' Program'],
 [],
 ["Total", '([0-9.]+).*Total Alias Queries Performed'],
 ["NoAlias", '([0-9.]+).*no alias responses'],
 ["MayAlias", '([0-9.]+).*may alias responses'],
 ["PartialAlias", '([0-9.]+).*partial alias responses'],
 ["MustAlias", '([0-9.]+).*must alias responses'],
 ["Symbols", '([0-9.]+).*# of symbols'],
 ["All", '([0-9.]+).*# of all values'],
 ["NoZeroes", '([0-9.]+).*# of all without noughts'],
 ["SameTree", '([0-9.]+).*Number of NoAlias from the same tree'],
 ["DiffRoots", '([0-9.]+).*Number of NoAlias from different roots'],
 ["SameBase", '([0-9.]+).*Number of Disjoint addresses from same base'],
 ["DiffBase", '([0-9.]+).*Number of Disjoint addresses from diff bases'],
 ["DiffArg", '([0-9.]+).*Number of Disjoint addresses from diff atg flags'],
 ["AddrCmp", '([0-9.]+).*Number of addres comparisons'],
 ["SB", '([0-9.]+).*Number of address cmp from same base'],
 [],
 );

